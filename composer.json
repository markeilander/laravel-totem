{
  "name": "eilander/laravel-totem",
  "description": "A Laravel package to manage your cron jobs through a beautiful dashboard",
  "keywords": [
    "laravel",
    "schedule",
    "manager"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "doctrine/dbal": "^2.9",
    "illuminate/bus": "^8.0",
    "illuminate/console": "^8.0",
    "illuminate/contracts": "^8.0",
    "illuminate/database": "^8.0",
    "illuminate/events": "^8.0",
    "illuminate/notifications": "^8.0",
    "laravelcollective/html": "^6.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/database": "^5.0",
    "orchestra/testbench" : "^5.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Eilander\\Totem\\": "src/",
      "Eilander\\Totem\\Tests\\": "tests/"
    }
  },
  "extra": {
    "component": "package",
    "laravel": {
      "providers": [
        "Eilander\\Totem\\Providers\\TotemServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
